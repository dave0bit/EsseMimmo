    <script type="text/javascript">

        function InvokeCSharpWithFormValues(elm) {

            var elements = document.getElementsByClassName('ms-ListItem is-selected');
            var i = elements.length;
            var menu = "";
            while (i--) {
                menu += elements[i].parentElement.id + ":" + elements[i].children[1].innerText + ",";
                alert(menu);
            }

            var qs = "";

            var elms = elm.form.elements;

            for (var i = 0; i < elms.length; i++) {
                qs += "&" + elms[i].name + "=" + elms[i].value;
                alert(qs);
            }

            qs += "&menu=" + menu;

            if (elms.length > 0)
                qs = qs.substring(1);

            location.href = "hybrid:" + elm.name + "?" + qs;
        }

        function SetLabelText(text) {
            var elm = document.getElementById('label');
            elm.innerHTML = text;
        }

    </script>

    <script type="text/javascript">
    $(document).ready(function() {

