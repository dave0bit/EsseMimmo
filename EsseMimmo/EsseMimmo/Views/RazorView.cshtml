        });

        $('.ms-Panel').FilePicker();
      } else {
        if ($.fn.ListItem) {
          $('.ms-ListItem').ListItem();
        }
      }

      if (typeof fabric !== "undefined") {
        if ('ListItem' in fabric) {
          var elements = document.querySelectorAll('.ms-ListItem');
          var i = elements.length;
          var component;
          while(i--) {
            component = new fabric['ListItem'](elements[i]);
          }
        }
      }

